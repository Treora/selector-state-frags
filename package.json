{
  "name": "selector-state-frags",
  "version": "0.2.0",
  "description": "Converter between the JSON and the URI+Fragment representation of Selectors and States",
  "main": "lib/index.js",
  "contributors": [
    {
      "name": "Ivan Herman",
      "email": "ivan@w3.org"
    },
    {
      "name": "Gerben",
      "email": "gerben@treora.com"
    }
  ],
  "scripts": {
    "build": "npm run pegjs && npm run transpile",
    "pegjs": "pegjs -o src/fragment.js src/fragment.pegjs",
    "prepublish": "npm run build",
    "postinstall": "postinstall-build lib",
    "test": "npm run pegjs && npm run test_src",
    "test_src": "mocha --compilers js:babel-register",
    "transpile": "babel src -d lib"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "mocha": "^3.3.0",
    "pegjs": "^0.10.0"
  },
  "dependencies": {
    "lodash.map": "^4.6.0",
    "postinstall-build": "^3.0.1"
  }
}
